    <!-- Plus Icon -->

    <!-- Microphone Icon -->

    <!-- Check Icon -->


    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="152dp"
        android:layout_height="201dp"
        android:layout_marginTop="224dp"
        android:alpha="0.2"
        android:src="@drawable/stng"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.543"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9"
        tools:ignore="ContentDescription,MissingConstraints" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:text="@string/hello_abcd"
        android:textColor="#262424"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/navigationView" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/welcome_to_juris_ai"
        android:textColor="#CC262424"
        android:textSize="25sp"

        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/search_bar"
        android:layout_width="0dp"
        android:layout_height="80dp"
        android:layout_margin="16dp"
        android:background="@color/white"
        android:gravity="center_vertical"
        android:hint="Search..."
        android:paddingStart="16dp"
        android:paddingEnd="50dp"
        android:textColor="#262424"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView14"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="Autofill,HardcodedText,TextFields" />

    <ImageButton
        android:id="@+id/imageView10"
        android:layout_width="52dp"
        android:layout_height="49dp"
        android:layout_marginEnd="16dp"
        android:backgroundTint="@color/white"
        app:layout_constraintBottom_toBottomOf="@+id/search_bar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/imageView11"
        app:layout_constraintTop_toTopOf="@+id/search_bar"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@android:drawable/ic_menu_send"
        app:tint="#262424"
        tools:ignore="ContentDescription,MissingConstraints" />

    <ImageButton
        android:id="@+id/imageView11"
        android:layout_width="40dp"
        android:layout_height="44dp"
        android:backgroundTint="@color/white"
        android:textColor="#eee5da"
        app:layout_constraintBottom_toBottomOf="@+id/search_bar"
        app:layout_constraintTop_toTopOf="@+id/search_bar"
        app:layout_constraintVertical_bias="0.861"
        app:srcCompat="@android:drawable/ic_btn_speak_now"
        app:tint="#262424"
        tools:ignore="ContentDescription,MissingConstraints"
        tools:layout_editor_absoluteX="329dp" />

    <ImageButton
        android:id="@+id/addButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:backgroundTint="@color/white"
        android:src="@android:drawable/ic_menu_add"
        android:textColor="#eee5da"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="@+id/search_bar"
        app:layout_constraintVertical_bias="1.0"
        app:tint="@color/black"
        tools:ignore="ContentDescription,MissingConstraints"
        tools:layout_editor_absoluteX="273dp" />

    <com.google.android.material.navigation.NavigationView
        android:id="@+id/navigationView"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:background="#262424"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:headerLayout="@layout/nav_header"
        app:menu="@menu/nav_menu">

    </com.google.android.material.navigation.NavigationView>

    <ImageButton
        android:id="@+id/imageView4"
        android:layout_width="56dp"
        android:layout_height="60dp"
        android:backgroundTint="#262424"
        android:textColor="#eee5da"
        app:layout_constraintStart_toStartOf="@+id/navigationView"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/ic_menu_sort_by_size"
        app:tint="#eee5da"
        tools:ignore="ContentDescription,MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>